{
  "all": true,
  "include": [
    "src/**"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/scripts/**",
    "src/database/migrations/*"
  ],
  "check-coverage": true,
  "watermarks": {
    "branches": [60,80],
    "functions": [60,80],
    "lines": [60,80],
    "statements": [60,80]
  },
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 60
}